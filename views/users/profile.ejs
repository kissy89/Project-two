<div id="greeting">
    <h2>Signed in as <%= user.username %> </h2>
</div>
<div id="user-places">
    <h2>Your Places: </h2>
</div>
<ul>
        <% user.places.forEach( (place) => { %>
          <li>
            <p class="item">
              <a><%= place.name %></a>
              <span><%= place.description %></span>
            </p>
          </li>
        <% }); %>
      </ul>